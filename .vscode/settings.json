{
  "[java]": {
    "editor.defaultFormatter": "redhat.java", // JavaのデフォルトフォーマッターとしてRed HatのJavaプラグインを使用
    "editor.formatOnSave": true, // 保存時に自動フォーマットを有効化
    "editor.formatOnPaste": true, // ペースト時に自動フォーマットを有効化
    "editor.formatOnType": true // タイプ時に自動フォーマットを有効化
  },
  "java.compile.nullAnalysis.mode": "automatic"
}
